---
layout: null
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//hacksw/handcal//NONSGML v1.0//EN

{% for event in site.data.events.events %}
{% if event.StartDate != "TBD" %}
BEGIN:VEVENT
UID:{{ event.Name | slugify }}
DTSTAMP:{{ "now" | date: "%Y%m%dT%H%M%S" }}
DTSTART;VALUE=DATE:{{ event.StartDate | date: "%Y%m%d" }}
{% if event.StartDate != event.EndDate %}
DTEND;VALUE=DATE:{{ event.EndDate | date: "%Y%m%d" }}
{% endif %}
SUMMARY:{{ event.Name }}
LOCATION:{{ event.Location }}
END:VEVENT
{% endif %}
{% endfor %}

END:VCALENDAR